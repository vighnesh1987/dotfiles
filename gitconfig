[core]
	editor = vim
[merge]
	tool = vimdiff
[color]
  ui = auto
[alias]
  conflicts = !git ls-files -u | cut -f 2 | sort -u

[push]
	default = simple
# This is to make `go get` use ssh
[url "git@github.com:"]
    insteadOf = https://github.com/

[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
